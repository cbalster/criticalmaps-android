{
    "problemMatcher": [
    {
      "owner": "errorprone-line",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):\\s+(warning|error):\\s+(?:\\[(.*)\\]\\s|)(.*)$",
          "file": 1,
          "line": 2,
          "severity": 3,
          "code": 4,
          "message": 5
        }
      ]
    }
  ]
}
